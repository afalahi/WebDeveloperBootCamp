<% include ./layouts/header %>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <% blogs.forEach(function(blog){ %>
      <div>
        <h2><%=blog.title%></h2>
        <img src="<%=blog.image%>">
        <span><%= blog.created %></span>
        <p><%=blog.body%></p>
      </div>
    <% }) %>
<% include ./layouts/footer %>